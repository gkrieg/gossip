<form action="/addPeer" method='post'>
  <div class="form-group">
    <label for="url">Peer URL</label>
    <input type="url" class="form-control" id="url" name="url" value="" placeholder="Peer URL">
  </div>
  <div class="form-group">
    <label for="name">Peer Name</label>
    <input type="text" class="form-control" id="name" name="name" value="" placeholder="Peer Name">
  </div>
  <button type="submit" class="btn btn-default">Add Peer</button>
</form>
